RewriteRule ^boulangeries-lille/?$ https://cmanima.com [R=301,L]
RewriteRule ^supermarches-lille/?$ https://cmanima.com [R=301,L]
RewriteRule ^mosquees-lille/?$ https://cmanima.com [R=301,L]
RewriteRule ^itineraires-lille/?$ https://cmanima.com [R=301,L]
RewriteRule ^avis-commerces/?$ https://cmanima.com [R=301,L]
